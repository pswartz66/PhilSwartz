{
  "name": "philswartz",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.1"
  },
  "description": "Single page portfolio website",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pswartz66/PhilSwartz.git"
  },
  "keywords": [],
  "author": "Phillip Swartz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pswartz66/PhilSwartz/issues"
  },
  "homepage": "https://github.com/pswartz66/PhilSwartz#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.1",
    "xoauth2": "^1.2.0"
  }
}
